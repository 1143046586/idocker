{
  "name": "docker-mobile",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn dev:web & yarn dev:server",
    "dev:web": "vite -c ./packages/web/vite.config.ts",
    "dev:server": "yarn build:server-w & nodemon ./packages/server/dist/index.cjs",
    "build:web": "cd ./packages/web && vite build",
    "build:server-w": "cd ./packages/server && vite build -w",
    "build:server": "cd ./packages/server && vite build",
    "build": "yarn build:server && yarn build:web",
    "image": "yarn build && docker buildx build -t evanfzq/docker-mobile:latest --platform=linux/arm,linux/arm64,linux/amd64 . --push",
    "start": "pm2-runtime start pm2.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.4",
    "@nestjs/core": "^10.2.4",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.4",
    "@nestjs/serve-static": "^4.0.0",
    "axios": "^1.5.0",
    "camelcase-keys": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "dockerode": "^3.3.5",
    "material-icons": "^1.13.11",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "vant": "^4.6.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.83",
    "@types/dockerode": "^3.3.19",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "electron-vite": "^1.0.27",
    "less": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.5",
    "vue-tsc": "^1.8.5"
  }
}
